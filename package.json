{
  "author": "DTrombett",
  "description": "A Node.js library to interact with the Clash Royale API",
  "homepage": "https://github.com/DTrombett/apiroyale#readme",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "name": "apiroyale",
  "types": "dist/index.d.ts",
  "version": "0.0.1",
  "keywords": [
    "nodejs",
    "api",
    "clash-royale-api",
    "clashroyale"
  ],
  "bugs": {
    "url": "https://github.com/DTrombett/apiroyale/issues"
  },
  "dependencies": {
    "@discordjs/collection": "^0.4.0",
    "@sapphire/async-queue": "^1.1.9",
    "tsup": "^5.11.11"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/node": "^16.11.20",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DTrombett/apiroyale.git"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/**/*.ts --fix && prettier --write src/**/*.ts",
    "precommit": "npm run lint",
    "postinstall": "npm run build",
    "start": "npm run build && node .",
    "test": "eslint src && tsc --noEmit"
  }
}