{
	"author": "DTrombett",
	"description": "A Node.js library to interact with the Clash Royale API",
	"homepage": "https://github.com/DTrombett/apiroyale#readme",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"name": "apiroyale",
	"types": "dist/index.d.ts",
	"version": "0.0.1",
	"keywords": [
		"nodejs",
		"api",
		"clash-royale-api",
		"clashroyale"
	],
	"scripts": {
		"build": "tsup && tsc --emitDeclarationOnly",
		"lint": "eslint src --fix && prettier --write src/**/*.ts",
		"precommit": "npm run lint",
		"postinstall": "npm run build",
		"start": "npm run build && node .",
		"test": "tsc --noEmit && eslint src"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DTrombett/apiroyale.git"
	},
	"bugs": {
		"url": "https://github.com/DTrombett/apiroyale/issues"
	},
	"dependencies": {
		"@discordjs/collection": "^0.5.0",
		"tsup": "^5.12.6"
	},
	"devDependencies": {
		"@tsconfig/node16": "^1.0.2",
		"@types/node": "^16.11.27",
		"@typescript-eslint/eslint-plugin": "^5.20.0",
		"@typescript-eslint/parser": "^5.20.0",
		"eslint": "^8.13.0",
		"husky": "^7.0.4",
		"prettier": "^2.6.2",
		"typescript": "^4.6.3"
	}
}
